{
  "name": "alice-telegram",
  "private": true,
  "version": "0.0.0",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "start": "wrangler pages dev --kv=USERS --compatibility-flags=nodejs_compat --compatibility-date=2024-01-29 ./public",
    "dev": "run-p dev:*",
    "dev:project": "remix dev --manual -c \"npm run start\"",
    "build": "remix build",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "tsc",
    "ts-check": "tsc --watch",
    "deploy-to-dev": "git push origin HEAD:feature/dev --force"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.5.0",
    "@remix-run/cloudflare": "^2.15.1",
    "@remix-run/cloudflare-pages": "^2.15.1",
    "@remix-run/css-bundle": "^2.15.1",
    "@remix-run/react": "^2.15.1",
    "@tanstack/react-query": "^5.62.7",
    "@tsndr/cloudflare-worker-jwt": "^3.1.3",
    "boolean": "^3.2.0",
    "commander": "^12.1.0",
    "daisyui": "^4.12.22",
    "debug": "^4.4.0",
    "isbot": "^5.1.18",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "posthog-node": "^4.3.2",
    "query-string": "^9.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.1.2",
    "rxjs": "^7.8.1",
    "tailwind-merge": "^2.5.5",
    "telegraf": "^4.16.3",
    "tiny-invariant": "^1.3.3",
    "ts-routes": "^2.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241216.0",
    "@remix-run/dev": "^2.15.1",
    "@types/express": "^5.0.0",
    "@types/google.accounts": "^0.0.15",
    "@types/node": "^20.14.8",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "autoprefixer": "^10.4.20",
    "esbuild-plugins-node-modules-polyfill": "file:packages/esbuild-plugins-node-modules-polyfill",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "lint-staged": "^15.2.11",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.16",
    "tsx": "^4.19.2",
    "typescript": "^5.5.4",
    "wrangler": "^3.83.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
